<Window x:Class="CpldUI.ResultPopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ResultPopUp" Height="460" Width="450" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Icon="../../logo.ico" WindowStyle="None">
    <Grid Name="GridResult">
        <Image Name="ImageResult"  Source="../../Icon/ok.bmp" Stretch="Fill"/>
        <Button Focusable="False" 
                Foreground="White" FontSize="18" 
                Background="#FFF74B4B" 
                Content="关 闭" x:Name="BtnClose" Height="48" Width="128"
                HorizontalAlignment="Center"  VerticalAlignment="Top"  
                Click="btnClose_Click" Margin="0,390,0,0"/>
    </Grid>
</Window>
